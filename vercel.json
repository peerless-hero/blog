{
  "version": 2,
  "builds": [],
  "routes": [
    {
      "src": "/(.*).(?:ico|gif|jpe?g|png|woff2?|eot|otf|ttf|svg|js|css|webmanifest)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "X-Content-Type-Options": "nosniff"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "Cache-Control": "no-cache",
        "X-Content-Type-Options": "nosniff"
      }
    },
    { "src": "/(.*)", "status": 404, "dest": "/404.html" }
  ]
}
